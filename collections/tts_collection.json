	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"const now = new Date();\r",
					"\r",
					"const yyyy = now.getFullYear();\r",
					"const mm = String(now.getMonth() + 1).padStart(2, '0');\r",
					"const dd = String(now.getDate()).padStart(2, '0');\r",
					"\r",
					"function generateUUID() {\r",
					"    return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {\r",
					"        const r = Math.random() * 16 | 0;\r",
					"        const v = c === 'x' ? r : (r & 0x3 | 0x8);\r",
					"        return v.toString(16);\r",
					"    });\r",
					"}\r",
					"\r",
					"const uuid = generateUUID();\r",
					"\r",
					"const ttsText =\r",
					"  `${yyyy}년 ${mm}월 ${dd}일자 데일리 테스트 진행합니다. ` +\r",
					"  `특이 사항 없습니다.`;\r",
					"\r",
					"pm.environment.set(\"tts_text\", ttsText);\r",
					"pm.environment.set(\"test_uuid\", uuid);\r",
					"\r",
					"// 로그 확인\r",
					"console.log(\"Generated text:\", ttsText);\r",
					"console.log(\"Generated uuid:\", uuid);"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"pm.test(\"Status code is 200\", function () {\r",
					"    pm.response.to.have.status(200);\r",
					"});\r",
					"\r",
					"pm.test(\"Response time < 60s\", function () {\r",
					"    pm.expect(pm.response.responseTime).to.be.below(60000);\r",
					"});\r",
					"\r",
					"pm.test(\"Response body exists\", function () {\r",
					"    pm.expect(pm.response.text()).to.not.be.empty;\r",
					"});"
				]
			}
		}
